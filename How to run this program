1. python face_dataset.py --dataset dataset
2. python face_aligner.py --dataset dataset
3. python encode_faces.py --dataset dataset_aligned --encodings output/encodings.pickle
4. python train_model.py --encodings output/encodings.pickle --recognizer output/recognizer.pickle --le output/le.pickle
5. python face_recog.py --recognizer output/recognizer.pickle --le output/le.pickle